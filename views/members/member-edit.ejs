    <div class="container">
 <div id="inside">
   <div id="center">
      <h1>Enter the Run Info</h1>
      <form method='POST' action='/members/<%= data.id %>?_method=PUT'>
        <div class="form-group">
          <label class="sr-only">First Name</label>
          <input type="text" class="form-control" name="first_name" title="First Name" value='<%= data.first_name %>' required>
        </div>
        <div class="form-group">
          <label class="sr-only">Last Name</label>
          <input type="text" class="form-control" name="last_name" title="Last Name" value='<%= data.last_name %>' required>
        </div>
        <div class="form-group">
          <label class="sr-only">Half Marathons</label>
          <input type="number" class="form-control" name="h_marathon" title="Half Marathons" value='<%= data.h_marathon %>' required>
        </div>
        <div class="form-group">
          <label class="sr-only">Full Marathons</label>
          <input type="number" class="form-control" name="f_marathon" title="Full Marathons" value='<%= data.h_marathon %>' required>
        </div>
        <div class="form-group">
          <label class="sr-only">Total Miles</label>
          <input type="text" class="form-control" name="t_miles" title="Total Miles" value='<%= data.t_miles%>' required>
        </div>
        <div class="form-group">
          <label class="sr-only">Updated Pictures</label>
          <input type="URL" class="form-control" name="pic" title="Updated Picture" value='<%= data.pic%>' required>
        </div>
        <input type="submit" value="SUBMIT">
      </form>
   </div>
 </div>
</div>

