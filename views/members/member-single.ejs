     <a href="/members/<%=data.id%>/run">View all Runs</a> 
    <a href="/members/<%=data.id%>/edit">Update Profile</a>
     <form method="POST" action="/members/<%= data.id %>?_method=DELETE">
        <input type='submit' value='DELETE PROFILE!!' />
    </form>
</div>