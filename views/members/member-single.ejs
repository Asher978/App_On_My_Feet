    <title>Members</title>
    <!-- Bootstrap -->
    <link href="/bootstrap/bootstrap.min.css" rel="stylesheet">
    <link href="/bootstrap/jumbotron.css" rel="stylesheet">
    <link rel='stylesheet' type='text/css' href='/styles/style.css'>
    <link href="https://fonts.googleapis.com/css?family=Faster+One" rel="stylesheet">
     <!-- <link href="../bootstrap/jumbotron-narrow.css" rel="stylesheet">  -->
    <link rel='stylesheet' type='text/css' href='/styles/jumbo.css'> 
  </head>
  <body>

    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <p id="log" class="navbar-brand">App On My Feet</p>   
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <div class="bs-example pull-right">
            <ul class="nav nav-pills">
                <li><a class="btn btn-info btn-lg" href="/user">Home</a></li>
                <li><a class="btn btn-info btn-lg" href="/members">Back</a></li>
                <li><a class="btn btn-info btn-lg" href="/auth/logout">Log Out</a></li>
        <li class="dropdown pull-right">
            <a href="#" data-toggle="dropdown" class="dropdown-toggle btn btn-info btn-lg">Other Members<b class="caret"></b></a>
            <ul class="dropdown-menu">
                <li><a href="/members/<%=data.id%>/addRun">Add a Run for current Member</a></li>
                <li><a href="/members/<%=data.id%>/run">View current Member's Runs</a></li>
                <li><a href="/members/<%=data.id%>/AddComment">Leave a Comment for this Member</a></li>
            </ul>
        </li>
        <li class="dropdown pull-right">
            <a href="#" data-toggle="dropdown" class="dropdown-toggle btn btn-info btn-lg">Current Members<b class="caret"></b></a>
            <ul class="dropdown-menu">
                <li><a href="/members/<%=data.id%>/edit">Update Profile</a></li>
                <li><a href="/members/<%=data.id%>/addRun">Add a Run for myself</a></li>
                <li><a href="/members/<%=data.id%>/run">View all my Runs</a></li>
                <li><a href="/members/<%=data.id%>/runCount">View all my Run Count</a></li>
                <li><a href="/members/<%=data.id%>/comment">Read my Comments</a></li>
                <li class="divider"></li>
                <li><a href="#">DELETE PROFILE</a></li>
            </ul>
        </li>
        </div><!--/.navbar-collapse -->
      </div>
      </div>
    </nav>  

<div id="single" class="container">
    <div class="card mb-3">
    <img id="memberPic" class="card-img-top" src="<%= data.pic%>" alt="Card image cap">
    <div class="card-block">
        <h2 class="card-title"><%= data.first_name %>  <span><%= data.last_name %></span></h2>
        <h4 class="card-text">Half-Marathons: <%= data.h_marathon %></h4>
        <h4 class="card-text">Full-Marathons: <%= data.f_marathon %></h4>
        <h4 class="card-text">Miles Run: <%= data.t_miles %></h4>
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: <%=data.t_miles/process.env.Goal*100%>%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%=data.t_miles/process.env.Goal*100%>%</div>
        </div>
    </div>
    </div>
<footer>
        <p>&copy; Ash 2017.</p>
      </footer>
</div>



    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script> 
